import java.util.HashSet;

class Solution {
    public boolean isUgly(int n) {
        if (n <= 0) return false;  // Ugly numbers are positive

        HashSet<Integer> set = new HashSet<>();
        int i = 2;

        while (n > 1) {
            if (n % i == 0) {
                n = n / i;        // keep dividing
                set.add(i);       // store factor
            } else {
                i++;              // try next divisor
            }

            // if factor is > 5 â†’ not ugly
            if (i > 5 ) {
                return false;
            }
        }

        // Now check if all collected factors are from {2, 3, 5}
        for (int factor : set) {
            if (factor != 2 && factor != 3 && factor != 5) {
                return false;
            }
        }

        return true;
    }
}
