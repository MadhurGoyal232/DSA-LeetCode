class Solution {
    public long countCompleteDayPairs(int[] hours) {
        HashMap<Integer, Integer> map = new HashMap<>();
        long count = 0;
         
        for(int i=0;i<hours.length;i++){
            int mod = hours[i] % 24;
            int r = (24 - mod) % 24;

            if(map.containsKey(r)){
                count+= map.get(r);
            }
            
            if(map.containsKey(mod)){
                map.put(mod , map.get(mod) +1);
                          
            }else{
                map.put(mod,1);
            }
            
            
        }

         return count;
    }
}
