class Solution {
    public int maxDepth(String s) {

        Stack<Character> stack = new Stack<>();
        int count = 0;
        int maxCount = 0;

        for(int i=0;i<s.length();i++){
            
            if(s.charAt(i) == '('){
                count++;
                maxCount = Math.max(count ,maxCount);
            }else if(s.charAt(i) == ')'){
                count--;
            }
        }
        

        return maxCount;
    }
}
