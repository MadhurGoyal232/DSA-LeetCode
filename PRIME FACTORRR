int n = sc.nextInt();
for(int i=2;i<=n;i++){

while(n%i == 0){
    System.out.println(i);
    n = n/i;
  }

}

OPTIMIZED APPROACH --------------------------------------------

for(int i=2;i*i<=n;i++){

while(n%i == 0){
    System.out.println(i);
    n = n/i;
  }

}
if(n!=1){
  System.out.println(n);      //////////eg 46,  2 and 23 are factors.
}
