class Solution {
    public int beautySum(String s) {

        int x = 0;

        for(int i=0;i<s.length();i++){
		    int freq[] = new int[26];

		    for(int j= i;j<s.length();j++){

                char c = s.charAt(j);
                freq[c-'a']++;                         

            
            
            if(j-i+1 >=2){
                int max = 0;
                int min = Integer.MAX_VALUE;
                for(int f : freq){

                    if(f>0){
                        
                        max = Math.max(max, f);
                        min = Math.min(min,f);
                    }
                }

                x+= max - min;
            }
            }

        }

        return x;
    }
}
