class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        
        if(matrix[0][0] > target){
            return false;
        }
        int y = 0;
        int r = 0;
        for(int i=0;i<matrix.length;i++){
            int len = matrix[i].length;
            if(matrix[i][0] <= target && matrix[i][len-1] >= target ){
                r = 1;
                y = i;
                break;
            }
        }

        if(r == 0){
            return false;
        }

        // for(int x=0;x<matrix.length;x++){


        // }
        int size = matrix[y].length;
        int left = 0;
        int right = size-1;

        while(left<= right){
            int mid = (left + right)/2;

            if(matrix[y][mid] == target){
                return true;
            }else if(matrix[y][mid] < target){
                left = mid+1;
            }else{
                right = mid-1;
            }
        }
        return false;
    }
}
