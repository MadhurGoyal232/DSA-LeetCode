class Solution {
    public int countCompleteDayPairs(int[] hours) {
        
        int i = 0;  
        int j = i + 1;
        int n = hours.length;       
        int count = 0;                  
                                                                                          
        while (i <= n - 2) {
            if ((hours[i] + hours[j]) % 24 == 0) {
                count++;
            }

            if (j == n - 1) {
                i++;
                if(i== n-1){
                    break;
                }
                j = i + 1;
            }else{
                j++;
            }
        }

        return count; 
    }
}
